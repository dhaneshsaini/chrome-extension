{
    "name": "Password Generator",
    "description": "Generate strong and unique password.",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon/16.png",
            "32": "icon/32.png"
        },
        "default_title": "Password Generator"
    },
    "icons": {
        "16": "icon/16.png",
        "32": "icon/32.png"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ]
}